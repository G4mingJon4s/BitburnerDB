{
	"extends": [
		"eslint:recommended",
		"next/core-web-vitals"
	],
	"rules": {
		"quotes": [
			"warn",
			"double"
		],
		"indent": [
			"error",
			"tab"
		],
		"jsx-quotes": [
			"error"
		],
		"curly": [
			"error",
			"multi"
		],
		"semi": [
			"error",
			"always"
		],
		"no-mixed-spaces-and-tabs": [
			"error"
		],
		"camelcase": [
			"error",
			{
				"properties": "always"
			}
		],
		"no-undef": [
			"off"
		],
		"no-multiple-empty-lines": [
			"warn",
			{
				"max": 1
			}
		]
	}
}